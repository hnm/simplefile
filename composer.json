	"description" : "Simple media module for n2n rocket",
	"keywords" : [
		"media",
		"n2n-tmpl-module",
		"n2n-module",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thomas GÃ¼nther",
			"email" : "guenther@hnm.ch"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": true,
